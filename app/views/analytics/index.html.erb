<div class="grid-container margin-top-4">
  <div class="grid-row">
    <h1>Total SORNs: <%= @total_sorns %></h1>
  </div>
  <div class="grid-row">
    <h2>SORN types:</h2>
    <ul>
    <% @sorn_types.each do |type, count| %>
      <li><%= type %>: <%= count %></li>
    <% end %>
    </ul>
  </div>

  <div class="grid-row">
    <h2>Agencies:</h2>
    <ul>
    <% @agencies.each do |agency| %>
      <li><%= agency.name %>: <%= agency.sorns.count %></li>
    <% end %>
    </ul>
  </div>



</div>
